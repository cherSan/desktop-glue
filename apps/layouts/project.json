{
  "name": "layouts",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/layouts/src",
  "prefix": "launchpad",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/layouts",
        "index": "apps/layouts/src/index.html",
        "main": "apps/layouts/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/layouts/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["apps/layouts/src/favicon.ico", "apps/layouts/src/assets"],
        "styles": [
          "node_modules/ng-zorro-antd/ng-zorro-antd.less",
          "node_modules/ng-zorro-antd/ng-zorro-antd.dark.less",
          "apps/layouts/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "layouts:build:production"
        },
        "development": {
          "browserTarget": "layouts:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4202
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "layouts:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/layouts/**/*.ts", "apps/layouts/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/layouts/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
